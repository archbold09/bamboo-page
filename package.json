{
  "name": "next-bamboo-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "cross-env NODE_ENV=production tailwindcss build styles/tailwind.css -o styles/styles.css",
    "dev-css": "cross-env NODE_ENV=development tailwindcss build styles/tailwind.css -o styles/styles.css",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run dev-css\"",
    "next-build": "next build",
    "build": "concurrently \"npm run next-build\" \"npm run build-css\"",
    "start": "next start"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "tailwindcss": "^1.7.5"
  }
}
